            </Code>
            <op:define prefix="LOG" header="SEN-UP1-8xTH.h" NumChannels="30" KoOffset="260" ModuleType="1" />
            <op:define prefix="THP" header="SEN-UP1-8xTH.h" NumChannels="8" KoOffset="20" ModuleType="2"/>
