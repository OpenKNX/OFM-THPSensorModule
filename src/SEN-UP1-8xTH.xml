              <op:include href="THPSensor.templ.xml" xpath="//Parameters/Parameter|//Parameters/Union" type="template" prefix="THP" />
            </Parameters>

            <ParameterRefs>
              <op:include href="../../OAM-LogicModule/src/Logikmodul.share.xml" xpath="//ParameterRefs/ParameterRef" prefix="LOG" />
